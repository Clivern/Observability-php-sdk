{
    "name": "clivern/observability-php-sdk",
    "type": "library",
    "description": "Observability SDK for PHP Applications.",
    "keywords": ["clivern", "observability"],
    "license": "MIT",
    "homepage": "https://github.com/clivern/observability-php-sdk",
    "authors": [
        {
            "name": "Clivern",
            "email": "hello@clivern.com",
            "homepage": "http://clivern.com"
        }
    ],
    "require": {
        "php":">=7.0",
        "ext-memcached": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.6.0",
        "phpunit/phpunit": "9.5.6",
        "friendsofphp/php-cs-fixer": "3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Clivern\\Observability\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
